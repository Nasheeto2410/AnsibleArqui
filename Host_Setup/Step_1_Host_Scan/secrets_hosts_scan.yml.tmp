ansible_become_pass: # Root password this machine (to use with 'become')
ansible_become_user: # User to become for privilege escalation (default is 'root')

inventory_group_name: # Name of the inventory group for the hosts scan

vlan_name: # Name of the VLAN being scanned (e.g., vlan10)
subnet: # Subnet to scan (e.g., 192.168.0.1/24)
ip_lower_limit: # Lower limit for IP addresses in the subnet (e.g., 50)
ip_upper_limit: # Upper limit for IP addresses in the subnet (e.g., 200)
